#include<stdio.h>
#include<string.h>

int main(){
	
	char yazi[]= "tura";
	
	printf("%d",strlen(yazi));   
	
/* "yazi" adresi veriyor. "strlen" stringin uzunluğunu veriyor
yani başlangıçtan null karakterine kadar stringin uzunluğunu basmayı sağlıyor */
	
	return 0;
	
}
