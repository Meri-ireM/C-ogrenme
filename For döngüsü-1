#include <stdio.h>
int main() {
	
	int i;
	
	for(i=0; i< 10; i++) {
	
	//döngünün değişkenini başlatmak gerekiyor. 0dan başlasın
	//sonrasında döngünün mantık cümlesi kurulmalı. döngü ne olduğu zaman devam etsin?
	// son olarak döngü ne kadar artmalı yani her döngünün dönüşünde i artsın istedim
	
	  printf("%d\n",i);
	
	
	
	
}

	return 0;
}

