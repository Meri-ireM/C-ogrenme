#include<stdio.h>

int main() {
	
	int sayilar[] = {1,2,3,4,5} ;
	int i= 0;
	
	for(; i<5; i++) {
		printf("%d\n",sayilar[i]);  //For döngüsü sayesinde tüm elemanları bastıracağız
		
	}
	
	return 0;
		
}
