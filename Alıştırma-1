#include <stdio.h>

int main()

{
	
	int a,b,c,d,e;
	
float aritmetik ;

printf("5 tane sayi giriniz : ");
scanf("%d %d %d %d %d",&a,&b,&c,&d,&e);
//kullanıcı her değer için enter a basmalı
aritmetik = (a+b+c+d+e)/5.0;
printf("girdiğiniz sayilarin aritmetikvortalamsi %.2f" , aritmetik);	
	
	
	
	return 0;
}

