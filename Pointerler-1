//Gercekte isletim sisteminin pointeri nereye koyduğunu bulma

#include <stdio.h>

int main(){
	
	int i= 5;
	
	int *p= &i;
	
	printf("%u", p);  // == %u ile %p sadece p hexadecimal sistemi ile gösterir
	
	return 0;
}

