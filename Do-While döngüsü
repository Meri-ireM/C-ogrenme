#include <stdio.h>

int main() {
	
	int n=10;
	
	do {
		
		printf("%d\n",n);
		
		n--;
	}
	while(n > 0); 
  
  //n 0'a gelene kadar döngü sağlanacak
	
	
	return 0;
}

